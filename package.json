{
  "name": "s-mqtt",
  "version": "1.0.0",
  "description": "S-MQTT NodeJS implementation\"",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc",
    "build-clean": "npm run clean && tsc",
    "postinstall": "npm run build",
    "go": "tsc && node dist/index.js",
    "dev": "nodemon -e ts  --exec \"npm run go\"",
    "watch": "tsc --watch",
    "test": "mocha --opts tests/mocha.opts",
    "test-watch": "mocha --opts tests/mocha.opts --watch",
    "clean": "rm -rf dist",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "lint-fix": "tslint -c tslint.json 'src/**/*.ts' --fix"
  },
  "devDependencies": {
    "@types/node": "^13.13.4",
    "@types/express": "^4.17.6",
    "tslint": "^6.1.1",
    "typescript": "^3.8.3",
    "mocha": "^7.1.1",
    "nodemon": "^2.0.3"
  },
  "keywords": [
    "MQTT",
    "mqtt",
    "security",
    "light security",
    "iot",
    "chacha20poly1305",
    "crypto",
    "secure communication",
    "secure iot"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pantalejmon/s-mqtt"
  }
}
